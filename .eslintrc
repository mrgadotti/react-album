{
    "extends": [
        "eslint-config-react-app"
    ],
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "import/no-unresolved": "off"
    },
    "settings": {
        "import/parsers": {
            "@typescript-eslint/parser": [
                ".ts",
                ".tsx"
            ]
        },
        "react": {
            "pragma": "React",
            "version": "detect"
        },
        "import/resolver": {
            "typescript": {
                "alwaysTryTypes": true
            }
        }
    }
}